generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

enum role {
  Cashier
  Admin
}

model User {
  id        Int       @id @default(autoincrement())
  name      String    @unique
  email     String
  password  String
  role      String    @default("Cashier")
  createdAt DateTime  @default(now())
}